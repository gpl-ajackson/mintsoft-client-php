{
    "name": "gpl-ajackson/mintsoft-client-php",
    "description": "A swagger generated PHP client for Mintsoft.",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "mintsoft"
    ],
    "homepage": "https://api.minstoft.com/v1/docs/",
    "license": "MIT",
    "authors": [
        {
            "name": "Ash Jackson",
            "homepage": "https://github.com/gpl-ajackson"
        },
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "squizlabs/php_codesniffer": "~4.0",
        "friendsofphp/php-cs-fixer": "~v3.0"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
